#Tipo de operadores de Lenguaje *Aritmenticos*

# Variables para uso Aritmetico

'''
my_suma = 0
my_resta = 0
my_multiplicacion = 0
my_division = 0
my_modulo = 0
my_exponencial = 0
my_division_entera = 0

my_variable_1 = 10
my_variable_2 = 5
my_variable_3 = 2

my_suma = my_variable_1 + my_variable_2
my_resta = my_variable_1 - my_variable_2
my_multiplicacion = my_variable_1 * my_variable_2
my_division = my_variable_1 / my_variable_2
my_modulo = my_variable_1 % my_variable_2
my_exponencial = my_variable_1 ** my_variable_2
my_division_entera = my_variable_1 // my_variable_2



print(f"el valor de la suma es {my_suma}") # f-string para imprimir el valor de la suma
print(f"el valor de la resta es {my_resta}") # f-string para imprimir el valor de la resta
print(f"el valor de la multiplicacion es {my_multiplicacion}") # f-string para imprimir el valor de la multiplicacion
print(f"el valor de la division es {my_division}") # f-string para imprimir el valor de la division
print(f"el valor del modulo es {my_modulo}") # f-string para imprimir el valor del modulo
print(f"el valor de la exponencial es {my_exponencial}") # f-string para imprimir el valor de la exponencial
print(f"el valor de la division entera es {my_division_entera}") # f-string para imprimir el valor de la division entera

'''

#Tipo de operadores de Lenguaje de *Asignacion*

# Variables para uso de Asignacion

'''
my_asig_suma = 0
my_asig_resta = 0
my_asig_multiplicacion = 0
my_asig_division = 0
my_asig_modulo = 0
my_asig_exponencial = 0
my_asig_division_entera = 0

my_asig_variable_1 = 10
my_asig_variable_2 = 5
my_asig_variable_3 = 2

my_asig_suma = 10
my_asig_suma += 5 # Asignacion de suma

my_asig_resta = 5
my_asig_resta -= 2 # Asignacion de resta

my_asig_multiplicacion = 2
my_asig_multiplicacion *= 5 # Asignacion de multiplicacion

my_asig_division = 10
my_asig_division /= 5 # Asignacion de division

my_asig_modulo = 10
my_asig_modulo %= 5 # Asignacion de modulo

my_asig_exponencial = 10
my_asig_exponencial **= 5 # Asignacion de exponencial

my_asig_division_entera = 10
my_asig_division_entera //= 5 # Asignacion de division entera

print(f"my asing suma es {my_asig_suma}")
print(f"my asing resta es {my_asig_resta}")
print(f"my asing multiplicacion es {my_asig_multiplicacion}")
print(f"my asing division es {my_asig_division}")
print(f"my asing modulo es {my_asig_modulo}")
print(f"my asing exponencial es {my_asig_exponencial}")
print(f"my asing division entera es {my_asig_division_entera}")

'''

#Tipo de operadores de Lenguaje de *Comparacion*

# Variables para uso de Comparacion

my_comp_variable_1 = 10
my_comp_variable_2 = 5
my_comp_variable_3 = 2
my_comp_variable_4 = 10

if my_comp_variable_1 == my_comp_variable_4:
    print(f"{my_comp_variable_1} es igual a {my_comp_variable_4}") # Comparacion de igualdad

if my_comp_variable_2 != my_comp_variable_3:
    print(f"{my_comp_variable_2} es diferente a {my_comp_variable_3}") # Comparacion de desigualdad

if my_comp_variable_4 > my_comp_variable_3:
    print(f"{my_comp_variable_4} es mayor que {my_comp_variable_3}") # Comparacion mayor que

if my_comp_variable_4 < my_comp_variable_3:
    print("Esto no se cumpliria") 
else:
    print(f"{my_comp_variable_4} es menor que {my_comp_variable_3}") # Comparacion menor que


if my_comp_variable_4 < my_comp_variable_3:
    print("Esto no se cumpliria")
elif my_comp_variable_4 <= my_comp_variable_3:
    print("Esto no se cumpliria")
elif my_comp_variable_4 >= my_comp_variable_3:
    print(f"{my_comp_variable_4} es mayor que {my_comp_variable_3}") # Comparacion menor o igual que
